{
  "name": "agentic-automation-framework",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "run:example": "node src/index.js --workflow ./examples/saucedemo-login.json --notify",
    "telegram:listen": "node src/index.js --telegram-listen",
    "plan": "node src/index.js --plan \"login to saucedemo and verify inventory\" --out ./examples/generated-workflow.json",
    "ask": "node src/index.js --ask \"login to saucedemo and confirm inventory page\"",
    "dashboard": "node src/index.js --dashboard",
    "test": "node tests/engine.test.js"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "playwright": "^1.56.1"
  }
}
